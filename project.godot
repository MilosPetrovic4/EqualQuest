; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Equal Quest"
run/main_scene="res://Scenes/Menus/Menu.tscn"
boot_splash/show_image=false
boot_splash/image="res://Art/Loading Screens/Boot Splash/loading.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://Art/Icon/icon.png"

[autoload]

Global="*res://Scripts/Standalone/Global.gd"
Positions="*res://Scripts/Standalone/Positions.gd"

[display]

window/size/width=960
window/size/height=540
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

fulls=false
fullscreen=false
wind=false
appli=false
resize=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
